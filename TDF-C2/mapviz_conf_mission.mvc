      topic: /drone114/ground_truth/pose
      color: "#a40000"
      draw_style: arrows
      position_tolerance: 0
      buffer_size: 1
      show_laps: false
      static_arrow_sizes: true
      arrow_size: 25
  - type: mapviz_plugins/path
    name: new display
    config:
      visible: true
